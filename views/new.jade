doctype html
html
  head
    meta(cherset='utf-8')
    meta(name="viewport", content="width=device-width, initial-scale=1")
    meta(name="viewport", content="width=device-width, initial-scale=1, shrink-to-fit=no")
    title Main
    style.
      .container {
      }
      .container-fluid {
      background-color: #ffffff;
      margin-top:20px;
      }
      .title {
      margin-left:20px;
      margin-right:20px;
      font-weight:bold;
      font-size:40px;
      color: #5b5b5b;
      }
      .col-xs-1 {
      margin-right:10px;
      }
      .button{
      margin-top:20px;
      }
    link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/css/bootstrap.min.css", integrity="sha384-/Y6pD6FV/Vv2HJnA6t+vslU6fwYXjCFtcEpHbNJ0lyAFsXTsjBbfaDjzALeQsN6M", crossorigin="anonymous")
  body

    .container-fluid
      .row
        .col-xs-1
          .title
            a(href="/") Overseas
        .col-xs-1
          .button
            button.btn.btn-outline-secondary.btn-sm(type="button") Exports
        .col-xs-1
          .button
            button.btn.btn-outline-secondary.btn-sm(type="button") Imports



    .container-fluid
      nav.navbar.navbar-expand-lg.navbar-light.bg-light
        a.navbar-brand(href="#") Menu
        button.navbar-toggler(type="button", data-toggle="collapse", data-target="#navbarSupportedContent", aria-controls="navbarSupportedContent", aria-expanded="false", aria-label="Toggle navigation")
          span.navbar-toggler-icon
        #navbarSupportedContent.collapse.navbar-collapse
          ul.navbar-nav.mr-auto
            li.nav-item
              a.nav-link(href="/list") Documents
            li.nav-item
              a.nav-link(href="#") PO Board
            li.nav-item
              a.nav-link(href="#") Items
            li.nav-item
              a.nav-link(href="#") Forwarder
            li.nav-item
              a.nav-link(href="#") Calender
          form.form-inline.my-2.my-lg-0
            input.form-control.mr-sm-2(type="text", placeholder="Search", aria-label="Search")
            button.btn.btn-outline-success.my-2.my-sm-0(type="submit") Search
      .container-fluid
        form(action='/',method='post')
          .form-group
            label(for="exampleFormControlInput1") PO Number
            |     
            input#exampleFormControlInput1.form-control(type="email", placeholder="ex.PO199999 (descriptions...)" name="po")
          |   
          .form-group
            label(for="exampleFormControlSelect1") Country select
            |     
            select(name="country")#exampleFormControlSelect1.form-control
              option USA (Play Visions)
              |       
              option China (Barter)
              |       
              option Europ (Goliath)
              |       
              option Canada (incredible novelties)
              |       
              option Japan (Tomy Takara)
          |   
          .form-group
            label(for="exampleFormControlSelect2") Condition
            |     
            select(name="condition" multiple="")#exampleFormControlSelect2.form-control
              option F.O.B. (Free On Board)
              |       
              option D.D.P. (Delivered Duty Paid)
              |       
              option C.I.F. (Cost,Insurance And Freight)
              |       
              option D.D.P. (Delivered Duty Paid)
              |       
              option D.D.U. (Delivered Duty Unpaid)
          |   
          .form-group
            label(for="exampleFormControlTextarea1") Note
            |     
            textarea(name="note" rows="3")#exampleFormControlTextarea1.form-control
          a(href="/")
            input(class="btn btn-default" type="submit" value="Submit")




    script(src="https://code.jquery.com/jquery-3.2.1.slim.min.js", integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN", crossorigin="anonymous")
    script(src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js", integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4", crossorigin="anonymous")
    script(src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js", integrity="sha384-h0AbiXch4ZDo7tp9hKZ4TsHbi047NrKGLO3SEJAg45jXxnGIfYzk4Si90RDIqNm1", crossorigin="anonymous")
